node {
   def app
   stage('Preparation') {
     checkout scm
   }
   stage('Build image') {         
     app = docker.build("bambino29/docker-node-demo")    
   }
   
   stage('Push image') {
      withCredentials([string(credentialsId: 'DockerHub', variable: 'DockerHub')]) {           
         sh 'docker login -u bambino29 -p ${DockerHub}'
         sh 'docker push bambino29/docker-node-demo'
     }    
   } 
}



